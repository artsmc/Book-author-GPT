{
  "name": "phoneangel-chrome-extension",
  "version": "0.5.0",
  "description": "A default scafolding for building Backend APIs with Angular 7 front-end.",
  "main": "index.js",
  "gypfile": true,
  "lint-staged": {
    "src/**/*.{ts,js,jsx,json,scss,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "scripts": {
    "heroku-prebuild": "npm install --dev",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test": "nyc mocha",
    "precommit": "lint-staged",
    "build-test": "ENV_CONFIG=testing webpack --config webpack.config.js",
    "build-prod": "ENV_CONFIG=production webpack --config webpack.config.js",
    "build": "webpack --config webpack.config.js --mode=development",
    "start": "npm-run-all --parallel watch:serve",
    "watch": "nodemon -e ts -w ./server -x npm run watch:server",
    "watch:server": "npm run build && node ./dist/server/server.js --PORT 3000",
    "watch:serve": "npm run watch"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "mapCoverage": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,jsx}",
      "!**/node_modules/**",
      "!**/client/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**/webpack.config.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^0.1.6",
    "aws-sdk": "^2.711.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "boom": "^7.3.0",
    "cachegoose": "^8.0.0",
    "chai": "^4.2.0",
    "compression-webpack-plugin": "^4.0.1",
    "connect-busboy": "0.0.2",
    "copy-webpack-plugin": "^6.0.3",
    "csv-parser": "^2.2.0",
    "epub": "^1.2.1",
    "express-rate-limit": "^5.5.1",
    "express-status-monitor": "^1.3.3",
    "file-chunked": "^1.0.4",
    "html-to-text": "^9.0.5",
    "http": "0.0.0",
    "https": "^1.0.0",
    "ip-regex": "^4.3.0",
    "joi": "^17.2.1",
    "mailgun-js": "^0.22.0",
    "mini-css-extract-plugin": "^0.11.0",
    "mongo": "^0.1.0",
    "mongodb": "^3.2.3",
    "mongoosastic": "^4.4.1",
    "mongoose": "^7.3.1",
    "mongoose-autopopulate": "^1.0.1",
    "mongoose-findoneorcreate": "^1.0.3",
    "node-fetch": "^2.6.12",
    "node-pre-gyp": "^0.15.0",
    "npm-watch": "^0.11.0",
    "openai": "^3.3.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "path": "^0.12.7",
    "pdf-parser": "^1.0.5",
    "pretty-ms": "^7.0.1",
    "s3": "^4.4.0",
    "sbd": "^1.0.19",
    "slug": "^1.1.0",
    "slugify": "^1.4.4",
    "textract": "^2.5.0",
    "tokgen": "^1.0.0",
    "typescript": "^3.9.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "wordcount": "^1.1.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.7.7",
    "@types/boom": "^7.2.1",
    "@types/express": "^4.17.17",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^26.0.4",
    "@types/json2csv": "^4.4.0",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.25",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.3.7",
    "@types/slug": "^0.9.1",
    "@types/voca": "^1.4.0",
    "dotenv": "^7.0.0",
    "esbuild": "^0.12.20",
    "esbuild-loader": "^2.15.0",
    "express": "^4.16.4",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "fs": "0.0.1-security",
    "graceful-fs": "4.2.2",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.4",
    "nodemon-webpack-plugin": "^4.5.2",
    "postcss": "^8.4.27",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "0.0.4",
    "rss-parser": "^3.13.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "ts-loader": "^5.4.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "webpack": "^5.42.0",
    "webpack-cli": "^3.3.12"
  },
  "resolutions": {
    "graceful-fs": "4.2.2"
  }
}
